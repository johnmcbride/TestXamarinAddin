<?xml version="1.0" encoding="UTF-8"?>
<Addin id          = "JohnTestTemplatesAddin"
       name        = "John Test Templates"
       author      = "John"
       description = "Project samples for testing"
       category    = "john"
       version     = "0.1">
<ExtensionModel>
	<Runtime>
		<Import file = "JohnTemplate.xpt.xml"/>
	</Runtime>
		<Dependencies>
		<Addin id = "Ide" version="4.2.3"/>
		<Addin id = "CSharpBinding" version = "4.2.3" />
	</Dependencies>

	<Extension path = "/MonoDevelop/Ide/ProjectTemplates">
		<ProjectTemplate id   = "JohnTemplate"
		                 file = "JohnTemplate.xpt.xml"/>
	</Extension>
</ExtensionModel>
	</Addin>
